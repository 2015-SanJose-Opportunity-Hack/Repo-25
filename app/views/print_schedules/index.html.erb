<p id="notice"><%= notice %></p>

<h1>Listing Print Schedules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @print_schedules.each do |print_schedule| %>
      <tr>
        <td><%= link_to 'Show', print_schedule %></td>
        <td><%= link_to 'Edit', edit_print_schedule_path(print_schedule) %></td>
        <td><%= link_to 'Destroy', print_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Print schedule', new_print_schedule_path %>
