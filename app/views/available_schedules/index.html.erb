<p id="notice"><%= notice %></p>

<h1>Listing Available Schedules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @available_schedules.each do |available_schedule| %>
      <tr>
        <td><%= link_to 'Show', available_schedule %></td>
        <td><%= link_to 'Edit', edit_available_schedule_path(available_schedule) %></td>
        <td><%= link_to 'Destroy', available_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Available schedule', new_available_schedule_path %>
